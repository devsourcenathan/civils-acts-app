{
  "compilerOptions": {
    "target": "es2017", // Assurez-vous que la cible est es2017 ou sup√©rieure
    "module": "commonjs",
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "outDir": "./dist",
    "rootDir": "./src",
    "strict": true,
    "moduleResolution": "node",
    "skipLibCheck": true
